@article{baiesi2017jpamt,
  title = {Exploring the Correlation between the Folding Rates of Proteins and the Entanglement of Their Native States},
  author = {Baiesi, Marco and Orlandini, Enzo and Seno, Flavio and Trovato, Antonio},
  year = {2017},
  month = dec,
  volume = {50},
  pages = {504001},
  issn = {1751-8113, 1751-8121},
  doi = {10.1088/1751-8121/aa97e7},
  abstract = {The folding of a protein towards its native state is a rather complicated process. However, there is empirical evidence that the folding time correlates with the contact order, a simple measure of the spatial organization of the native state of the protein. Contact order is related to the average length of the main chain loops formed by amino acids that are in contact. Here we argue that folding kinetics can also be influenced by the entanglement that loops may undergo within the overall three-dimensional protein structure. In order to explore such a possibility, we introduce a novel descriptor, which we call `maximum intrachain contact entanglement'. Specifically, we measure the maximum Gaussian entanglement between any looped portion of a protein and any other non-overlapping subchain of the same protein, which is easily computed by discretized line integrals on the coordinates of the C{$\alpha$} atoms. By analyzing experimental data sets of two-state and multi-state folders, we show that the new index is also a good predictor of the folding rate. Moreover, being only partially correlated with previous methods, it can be integrated with them to yield more accurate predictions.},
  file = {/home/leonardo/zotero-library/AUTH/Baiesi/baiesi_2017_exploring_the_correlation_between_the_folding_rates_of_proteins_and_the.pdf},
  journal = {Journal of Physics A: Mathematical and Theoretical},
  language = {en},
  number = {50}
}

@article{dang2020cs,
  title = {Cellular {{Dialogues}}: {{Cell}}-{{Cell Communication}} through {{Diffusible Molecules Yields Dynamic Spatial Patterns}}},
  shorttitle = {Cellular {{Dialogues}}},
  author = {Dang, Yiteng and Grundel, Douwe A.J. and Youk, Hyun},
  year = {2020},
  month = jan,
  volume = {10},
  pages = {82-98.e7},
  issn = {24054712},
  doi = {10.1016/j.cels.2019.12.001},
  abstract = {Cells form spatial patterns by coordinating their gene expressions. How a group of mesoscopic numbers (hundreds to thousands) of cells, without pre-existing morphogen gradients and spatial organization, self-organizes spatial patterns remains poorly understood. Of particular importance are dynamic spatial patterns such as spiral waves that perpetually move and transmit information. We developed an open-source software for simulating a field of cells that communicate by secreting any number of molecules. With this software and a theory, we identified all possible ``cellular dialogues''\textemdash ways of communicating with two diffusing molecules\textemdash that yield diverse dynamic spatial patterns. These patterns emerge despite widely varying responses of cells to the molecules, gene-expression noise, spatial arrangements, and cell movements. A three-stage, ``order-fluctuate-settle'' process forms dynamic spatial patterns: cells form long-lived whirlpools of wavelets that, following erratic dynamics, settle into a dynamic spatial pattern. Our work helps in identifying generegulatory networks that underlie dynamic pattern formations.},
  file = {/home/leonardo/zotero-library/AUTH/Dang/dang_2020_cellular_dialogues_-_cell-cell_communication_through_diffusible_molecules_yields.pdf},
  journal = {Cell Systems},
  language = {en},
  number = {1}
}

